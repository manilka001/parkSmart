generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")

}

model User {
  id          String   @id
  email       String   @unique
  firstName   String
  lastName    String
  street      String
  city        String
  state       String
  zipCode     String
  contactNo   String
  coordinates String?
  createdAt   DateTime @default(now())
}
